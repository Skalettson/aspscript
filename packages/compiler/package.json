{
  "name": "@aspscript/compiler",
  "version": "1.3.0",
  "description": "AspScript compiler - transforms .aspc files to optimized JavaScript",
  "main": "index.js",
  "type": "module",
  "bin": {
    "aspc": "./bin/aspc.js"
  },
  "exports": {
    ".": {
      "import": "./index.js",
      "require": "./index.cjs"
    },
    "./bundle-analyzer": {
      "import": "./bundle-analyzer.js"
    },
    "./incremental": {
      "import": "./incremental.js"
    },
    "./treeshaker": {
      "import": "./treeshaker.js"
    }
  },
  "scripts": {
    "build": "echo 'No build needed for compiler'",
    "test": "node --test",
    "dev": "node --watch index.js"
  },
  "keywords": [
    "aspscript",
    "compiler",
    "build"
  ],
  "author": "Adel Petrov",
  "license": "MIT",
  "dependencies": {
    "acorn": "^8.12.1",
    "acorn-jsx": "^5.3.2",
    "astring": "^1.9.0",
    "css-tree": "^2.3.1",
    "postcss": "^8.4.41",
    "sass": "^1.70.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "rollup": "^4.24.0"
  }
}
